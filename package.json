  "scripts": {
    "prebuild": "rimraf dist",
    "seed": "node -r ts-node/register --experimental-modules ./scripts/seed/app.ts",
    "build": "nest build --path ./tsconfig.server.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start --path ./tsconfig.server.json",
    "start:client": "next dev -p 3001",
    "start:debug": "nest start --debug --watch --path ./tsconfig.server.json",
    "start:prod": "node dist/src/main",
    "doc": "compodoc -p tsconfig.json -s",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
